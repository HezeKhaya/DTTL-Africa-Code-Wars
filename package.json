{
  "name": "next-app",
  "version": "2.0.14",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint .",
    "test": "vitest",
    "type-check": "tsc",
    "generate-barrels": "barrelsby --delete --config ./barrelsby.json",
    "generate-theme": "npx @chakra-ui/cli typegen ./theme.ts",
    "generate-db-types": "npx supabase gen types --linked --lang=typescript > database/types.ts",
    "prebuild": "npm run generate-barrels && npm run generate-theme && npm run lint"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@chakra-ui/react": "*",
    "@emotion/react": "^11.14.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.2",
    "dayjs": "^1.11.18",
    "kysely": "^0.28.7",
    "kysely-postgres-js": "^3.0.0",
    "next": "^15.3.4",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "5.5.0",
    "remeda": "^2.32.0",
    "zod": "^4.1.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chakra-ui/chakra-ui.git",
    "directory": "examples/next-app"
  },
  "bugs": {
    "url": "https://github.com/chakra-ui/chakra-ui/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@chakra-ui/cli": "^3.27.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "barrelsby": "^2.8.1",
    "jsdom": "^27.0.0",
    "kysely-supabase": "^0.2.1",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
